{
  "name": "gpti-data-bot",
  "version": "1.0.0",
  "description": "GPTI Data Bot - Phase 3 API integrations",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:cache": "jest src/cache/redis-client.test.ts",
    "test:limiter": "jest src/utils/rate-limiter.test.ts",
    "test:sss": "jest src/agents/sss/sss-sanctions.agent.test.ts",
    "build": "tsc",
    "dev": "tsc --watch",
    "load-test": "ts-node scripts/load-test.ts",
    "test:fca-api": "ts-node scripts/test-fca-api.ts",
    "db:schema": "psql -U postgres -d gpti_sanctions -f database/schema.sql",
    "download:ofac": "ts-node scripts/download-ofac.ts",
    "download:un": "ts-node scripts/download-un.ts",
    "etl:sanctions": "ts-node scripts/etl-sanctions.ts",
    "etl:ofac": "ts-node scripts/etl-sanctions.ts --ofac-only",
    "etl:un": "ts-node scripts/etl-sanctions.ts --un-only",
    "etl:force": "ts-node scripts/etl-sanctions.ts --force",
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "start:mock": "MOCK_MODE=true ts-node src/index.ts",
    "test:api": "jest src/api/verification-api.mock.test.ts",
    "test:connectivity": "ts-node src/utils/internet-connectivity-test.ts",
    "agents:start": "ts-node src/api/agents-server.ts",
    "agents:mock": "MOCK_MODE=true ts-node src/api/agents-server.ts"
  },
  "dependencies": {
    "@types/pg": "^8.16.0",
    "@types/redis": "^4.0.10",
    "@types/xml2js": "^0.4.14",
    "axios": "^1.13.4",
    "body-parser": "^1.20.2",
    "cheerio": "^1.2.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "express": "^4.18.2",
    "ioredis": "^5.9.2",
    "pg": "^8.18.0",
    "redis": "^5.10.0",
    "string-similarity": "^4.0.4",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.1.0",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "jest": "^30.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
